{
  "name": "@releaseband/eslint-config",
  "version": "0.0.0-semantic-release",
  "description": "eslint shareable config",
  "author": "releaseband",
  "license": "MIT",
  "main": "index.js",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "commit": "cz",
    "format": "prettier --write --ignore-path ./.gitignore .",
    "lint": "eslint --ext .js --ignore-path ./.gitignore .",
    "lint:fix": "npm run lint --  --fix"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^15.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/cz-commitlint": "^16.1.0",
    "@releaseband/commitlint-config": "^0.2.0",
    "@releaseband/prettier-config": "^0.2.0",
    "@releaseband/semantic-release-npm-github-config": "^1.0.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "prettier": "^2.5.1"
  },
  "peerDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/releaseband/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/releaseband/eslint-config/issues"
  },
  "homepage": "https://github.com/releaseband/eslint-config#readme",
  "keywords": [
    "eslint",
    "eslint-config"
  ]
}
